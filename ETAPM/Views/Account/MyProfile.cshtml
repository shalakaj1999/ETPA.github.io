@model ETAPM.Models.AppUser

@{
    ViewBag.Title = "Dashboard";
    ViewBag.PageNav = "<a class='page-nav' href='" + Url.Action("", "Dashboard") + "'>Dashboard</a><i class='page-nav-divider'></i><span class='page-nav-current'>Employees</span>";

    Layout = "~/Views/Shared/_layout.cshtml";
}

<div class="container-fluid">

    <div class="display-4">My Profile</div>

    <div class="badge badge-warning mt-3">@Model.AppRole.AppRoleName</div>
    <div class="form-row" style="max-width:400px;">
        <div class="form-group col-6">
            <label class="form-label">First name</label>
            <div class="form-control-label">@Model.FirstName</div>
        </div>
        <div class="form-group col-6">
            <label class="form-label">Last name</label>
            <div class="form-control-label">@Model.LastName</div>
        </div>
    </div>
    <div class="form-row" style="max-width:400px;">
        <div class="form-group col-6">
            <label class="form-label">Mobile</label>
            <div class="form-control-label">@Model.MobileNo</div>
        </div>
        <div class="form-group col-6">
            <label class="form-label">Email</label>
            <div class="form-control-label">@Model.EmailId</div>
        </div>
    </div>

</div>